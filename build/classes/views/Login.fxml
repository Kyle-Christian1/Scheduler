<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.LoginController">
   <children>
      <Label fx:id="programTitleLabel" layoutX="84.0" layoutY="41.0" text="Scheduler Application">
         <font>
            <Font size="24.0" />
